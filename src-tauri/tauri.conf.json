{
   "$schema": "https://schema.tauri.app/config/2",
   "productName": "desktop_app",
   "version": "0.1.0",
   "identifier": "com.kacper.desktop-app",
   "build": {
      "beforeDevCommand": "npm run dev",
      "devUrl": "http://localhost:1420",
      "beforeBuildCommand": "npm run build",
      "frontendDist": "../dist"
   },
   "app": {
      "windows": [
         {
            "title": "UÅ‚as app",
            "width": 1400,
            "height": 900,
            "minWidth": 1200,
            "minHeight": 700,
            "center": true,
            "resizable": true
         }
      ],
      "security": {
         "csp": null
      }
   },
   "bundle": {
      "resources":[
         ".env.production"
      ],
      "active": true,
      "targets": "all",
      "icon": [
         "icons/32x32.png",
         "icons/128x128.png",
         "icons/128x128@2x.png",
         "icons/icon.icns",
         "icons/icon.ico"
      ]
   }
}
